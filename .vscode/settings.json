{
  "java.project.sourcePaths": ["build/generated-src/antlr/main/"],
  "[java]": {
    "editor.suggest.snippetsPreventQuickSuggestions": false
  },
  "files.associations": {
    "*.snap": "glsl",
    "*.cases": "glsl",
    "**/glslang-test/*": "glsl"
  },
  "coverage-gutters.coverageFileNames": [
    "lcov.info",
    "cov.xml",
    "coverage.xml",
    "jacoco.xml",
    "jacocoTestReport.xml"
  ],
  "java.import.gradle.java.home": "/Library/Java/JavaVirtualMachines/jdk-16.0.1.jdk/Contents/Home",
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-16",
      "path": "/Library/Java/JavaVirtualMachines/jdk-16.0.1.jdk/Contents/Home",
      "default": true
    }
  ],
  "coverage-gutters.coverageBaseDir": "glsl-transformer/build/reports/jacoco/test",
  "java.sources.organizeImports.staticStarThreshold": 1,
  "java.sources.organizeImports.starThreshold": 2,
  "editor.detectIndentation": false,
  "editor.insertSpaces": true
}
