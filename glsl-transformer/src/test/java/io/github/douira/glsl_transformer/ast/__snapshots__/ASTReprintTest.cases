	executeCallableEXT(callableSbtIndex, 0);
	imgColor = payload + vec4(blendColor);
	imageStore(img, ivec2(gl_LaunchIDEXT), imgColor);
	ignoreIntersectionEXT;
	terminateRayEXT;
}

